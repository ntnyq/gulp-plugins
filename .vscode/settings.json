{
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "vue",
    "html",
    "json",
    "jsonc",
    "yaml",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.words": [
    "codecov",
    "lcov",
    "noopener",
    "pnpm",
    "starptech",
    "tsbuildinfo",
    "tsup",
    "vitest"
  ]
}
