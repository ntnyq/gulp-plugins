{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "eslint.validate": [
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml",
    "json5",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "[html]": {
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "noopener",
    "pnpm",
    "starptech",
    "tsbuildinfo",
    "tsup",
    "vitest"
  ]
}
